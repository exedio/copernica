
<Context
		path="/copetest-@DATABASE@"
		docBase="../../persistence/copernica/build/testwebapp-@DATABASE@"
		debug="0"
		privileged="true">
	<Logger
			className="org.apache.catalina.logger.FileLogger"
			prefix="localhost_copetest_@DATABASE@_log."
			suffix=".txt"
			timestamp="true"/>

	<!-- Disables session persistence in SESSIONS.ser -->
	<Manager
			className="org.apache.catalina.session.StandardManager"
			pathname="" />

</Context>
